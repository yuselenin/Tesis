author = {Ferrerira, S{\'{e}}rgio Dominique and Rial, Antonio and Varela, Jes{\'{u}}s},
doi = {10.1097/CCM.0b013e318256b800},
file = {:home/concytec/.local/share/data/Mendeley Ltd./Mendeley Desktop/Downloaded/Ferrerira, Rial, Varela - 2010 - Segmentacion post hoc del mercado tur{\'{i}}stico espa{\~{n}}ol.pdf:pdf},
issn = {1851-1732},
